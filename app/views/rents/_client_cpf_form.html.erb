<div class="row">
  <div class="col-lg-12">
    <%= simple_form_for @client, url: new_rent_path, method: "GET", defaults: form_defaults do |f| %>
      <%= f.error_notification %>

      <div class='form-inputs'>
        <%= f.input :cpf %>
      </div>

      <div class='form-actions pull-right'>
        <%= f.button :submit, 'Vincular CPF', class: 'btn-primary' %>
      </div>
    <% end %> 
  </div>
</div>